                {
                        if ( $(window).height() > $("#contenu").outerHeight( true ) )
                        {
                                footertop = $(window).height() - $("#footer").outerHeight( true );
                        }
                        else
                        {
                                footertop = $("#contenu").outerHeight( true );
                        }
                        
                        $("#footer").css('top', footertop + "px");
                }
 
        </script>
